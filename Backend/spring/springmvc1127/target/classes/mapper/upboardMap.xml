<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="upboard">
	<insert id="add" parameterType="upvo">
		insert into upboard values(
		upboard_seq.nextVal,#{title},#{writer},#{content},
		#{imgn},0,#{reip},sysdate
		)
	</insert>
	<select id="list" resultType="upvo">
		SELECT * from upboard ORDER BY 1 DESC
	</select>
	<select id="totalCount" resultType="int">
		select count(*) cnt from upboard
	</select>
</mapper>    


